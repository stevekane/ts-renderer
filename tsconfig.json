{
  "compilerOptions": {
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallThroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicityReturns": true,
    "noImplicitThis": true,
    "target": "es6"
  },
  "exclude": [
    "node_modules",
    "test"
  ]
}
